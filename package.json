{
  "name": "react_native_programm",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest --no-cache --runInBand",
    "build-debug-apk": "cd android && ./gradlew assembleDebug",
    "build-release-apk": "cd android && ./gradlew assembleRelease --warning-mode all",
    "storybook": "NODE_OPTIONS=--openssl-legacy-provider start-storybook -p 7007",
    "build-storybook": "build-storybook",
    "prestorybook": "rnstl",
    "prepare": "husky install",
    "commit": "npm run lint && git add . && git-cz",

    "android:dev": "react-native run-android --variant=devDebug --appIdSuffix=reactnativeprogramm.dev",
    "ios:dev": "react-native run-ios --scheme ReactNativeProgramm-dev",
    "android:qa": "react-native run-android --mode=qaDebug --appIdSuffix=reactnativeprogramm.qa",
    "ios:qa": "react-native run-ios --scheme ReactNativeProgramm-qa",
    "android:prod": "react-native run-android --mode=prodDebug --appIdSuffix=reactnativeprogramm.prod",
    "ios:prod": "react-native run-ios --scheme ReactNativeProgramm-prod"
  },
  "dependencies": {
    "@expo/vector-icons": "^13.0.0",
    "@react-native-community/async-storage": "^1.12.1",
    "@react-native-community/cli": "^11.3.6",
    "@react-native-community/cli-platform-android": "^11.3.6",
    "@react-native-community/masked-view": "^0.1.11",
    "@react-native/codegen": "^0.72.6",
    "@react-navigation/drawer": "^6.6.3",
    "@react-navigation/native": "^6.1.7",
    "@react-navigation/native-stack": "^6.9.13",
    "@react-navigation/stack": "^6.3.17",
    "@rneui/base": "^4.0.0-rc.7",
    "@rneui/themed": "^4.0.0-rc.7",
    "appcenter": "5.0.0",
    "appcenter-analytics": "5.0.0",
    "appcenter-crashes": "5.0.0",
    "babel-jest": "^29.6.4",
    "enzyme": "^3.11.0",
    "expo": "^44.0.6",
    "expo-font": "^11.4.0",
    "expo-secure-store": "~12.3.1",
    "jest": "^29.1.1",
    "moment": "^2.29.4",
    "react": "18.2.0",
    "react-geocode": "^0.2.3",
    "react-native": "0.72.1",
    "react-native-codegen": "^0.71.5",
    "react-native-config": "^1.5.1",
    "react-native-elements": "^3.4.3",
    "react-native-geolocation-service": "^5.3.1",
    "react-native-gesture-handler": "^2.12.0",
    "react-native-image-picker": "^5.6.0",
    "react-native-maps": "^1.7.1",
    "react-native-reanimated": "^3.3.0",
    "react-native-screens": "^3.22.1",
    "react-native-share": "^9.2.3",
    "react-native-snap-carousel": "^1.3.1",
    "react-native-svg": "^13.10.0",
    "react-native-vector-icons": "^9.2.0",
    "ts-jest": "^29.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.22.11",
    "@babel/preset-env": "^7.20.0",
    "@babel/preset-react": "^7.22.5",
    "@babel/runtime": "^7.20.0",
    "@react-native-async-storage/async-storage": "^1.19.3",
    "@react-native-community/datetimepicker": "^7.5.0",
    "@react-native-community/slider": "^4.4.3",
    "@react-native/eslint-config": "^0.72.2",
    "@react-native/metro-config": "^0.72.7",
    "@storybook/addon-actions": "^7.4.0",
    "@storybook/addon-knobs": "^7.0.2",
    "@storybook/addon-links": "^5.3.7",
    "@storybook/addon-ondevice-actions": "^5.3.23",
    "@storybook/addon-ondevice-knobs": "^6.5.6",
    "@storybook/react-native": "^5.3.23",
    "@storybook/react-native-server": "^6.5.6",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^12.2.2",
    "@tsconfig/react-native": "^3.0.0",
    "@types/metro-config": "^0.76.3",
    "@types/react": "^18.0.24",
    "@types/react-geocode": "^0.2.1",
    "@types/react-test-renderer": "^18.0.0",
    "babel-loader": "^8.3.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.19.0",
    "husky": "^8.0.3",
    "metro-react-native-babel-preset": "0.76.5",
    "prettier": "^2.4.1",
    "react-dom": "^18.2.0",
    "react-native-dotenv": "^3.4.9",
    "react-native-safe-area-context": "^4.7.1",
    "react-native-storybook-loader": "^2.0.5",
    "react-test-renderer": "18.2.0",
    "typescript": "4.8.4"
  },
  "engines": {
    "node": ">=16"
  },
  "config": {
    "react-native-storybook-loader": {
      "pattern": "**/*.stories.tsx",
      "outputFile": "./storybook/storyLoader.js"
    }
  }
}
